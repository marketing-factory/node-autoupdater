{
  "typescript.check.tscVersion": false,
  "files.exclude": {
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.map": {
      "when": "$(basename).map"
    }
  },
  "cSpell.words": [
    "autoupdate",
    "AUTOUPDATER",
    "execa",
    "gitbeaker",
    "Jikgcv",
    "memfs",
    "toplevel"
  ],
  "cSpell.enableFiletypes": [
    "!cpp",
    "!go",
    "!graphql",
    "!handlebars",
    "!haskell",
    "!jade",
    "!java",
    "!latex",
    "!pug",
    "!rust",
    "!scala",
    "jsx",
    "powershell"
  ],
  "cSpell.ignoreWords": [
    "Buildx",
    "softprops",
    "subproject"
  ]
}